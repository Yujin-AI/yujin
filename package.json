{
  "name": "yujin",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --watch",
    "predev": "docker compose -f docker/dev/docker-compose.yml up -d",
    "test:c8": "c8 node ace test",
    "test": "node ace test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "depcheck": "depcheck",
    "job:spider": "node ace resque:start --schedule=false --worker --is-multi --queue-name=spider",
    "job:shopify": "node ace resque:start --schedule=false --worker --is-multi --queue-name=shopify_processor",
    "job:scheduler": "node ace resque:start --schedule --worker=false"
  },
  "imports": {
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#validators/*": "./app/validators/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#lib/*": "./app/lib/*.js",
    "#jobs/*": "./app/jobs/*.js",
    "#decorators/*": "./app/decorators/*.js",
    "#provider": "./providers/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^7.5.2",
    "@adonisjs/eslint-config": "^1.3.0",
    "@adonisjs/prettier-config": "^1.3.0",
    "@adonisjs/tsconfig": "^1.3.0",
    "@japa/api-client": "2.0.3",
    "@japa/assert": "^3.0.0",
    "@japa/plugin-adonisjs": "^3.0.1",
    "@japa/runner": "^3.1.4",
    "@swc/core": "^1.5.7",
    "@types/luxon": "^3.4.2",
    "@types/node": "^20.12.12",
    "@types/turndown": "^5.0.4",
    "@types/uuid": "^9.0.8",
    "c8": "9.1.0",
    "depcheck": "1.4.7",
    "eslint": "^9.2.0",
    "hot-hook": "0.2.6",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@adonisjs/auth": "^9.2.1",
    "@adonisjs/core": "^6.9.0",
    "@adonisjs/cors": "^2.2.1",
    "@adonisjs/lucid": "^20.6.0",
    "@adonisjs/redis": "^8.0.1",
    "@dqbd/tiktoken": "1.0.15",
    "@vinejs/vine": "^2.0.0",
    "adonis-autoswagger": "^3.22.0",
    "adonis-resque": "1.2.2",
    "cheerio": "1.0.0-rc.12",
    "crawlee": "3.9.2",
    "luxon": "^3.4.4",
    "openai": "4.47.1",
    "pg": "^8.11.5",
    "playwright": "1.44.0",
    "reflect-metadata": "^0.2.2",
    "turndown": "7.1.3",
    "typesense": "1.8.2",
    "uuid": "9.0.1"
  },
  "hotHook": {
    "boundaries": [
      "./app/controllers/**/*.ts",
      "./app/middleware/*.ts"
    ]
  },
  "eslintConfig": {
    "extends": "@adonisjs/eslint-config/app"
  },
  "prettier": "@adonisjs/prettier-config",
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195"
}
